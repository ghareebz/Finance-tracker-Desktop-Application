<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="javafxapplication1.ViewTransactionsController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label prefHeight="31.0" prefWidth="210.0" text="All Transactions">
               <font>
                  <Font name="Arial Black" size="21.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="200.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets left="8.55" />
               </padding>
            </Label>
            <TableView fx:id="transactionTable" prefHeight="365.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="typeColumn" prefWidth="120.0" text="Type" />
                <TableColumn fx:id="amountColumn" prefWidth="120.0" text="Amount" />
                  <TableColumn fx:id="dateColumn" prefWidth="120.0" text="Date" />
                  <TableColumn fx:id="categoryColumn" prefWidth="120.0" text="Category" />
                  <TableColumn fx:id="noteColumn" prefWidth="120.0" text="Note" />
              </columns>
            </TableView>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" text="Back">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
               <VBox.margin>
                  <Insets left="250.0" top="12.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
